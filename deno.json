{
  "imports": {
    "chalk": "https://esm.sh/chalk@5.2.0",
    "dompurify": "https://esm.sh/dompurify@3.0.3",
    "dotenv/": "https://deno.land/std@0.188.0/dotenv/",
    "deno_dom/": "https://deno.land/x/deno_dom@v0.1.38/",
    "fs-extra": "npm:fs-extra@11.1.1",
    "js-beautify": "https://esm.sh/js-beautify@1.14.8",
    "mkdirp": "https://esm.sh/mkdirp@3.0.1",
    "node-fetch": "https://esm.sh/node-fetch@3.3.1"
  },
  "tasks": {
    "dev": "cd ./_site && deno run --allow-read=./ --allow-net=0.0.0.0:4000 https://deno.land/std@0.188.0/http/file_server.ts  ./ --port=4000",
    "build": "deno task download:images && deno task build:pages",

    "build:pages": "deno run --allow-env --allow-read=./ --allow-write=./_api,./_site --allow-sys=uid --allow-net=content.dollyskettle.com build-deno.js",
    "download:data": "deno run --allow-env --allow-read=./ --allow-write=./_api --allow-net=content.dollyskettle.com download-data-deno.js",
    "download:images": "deno run --allow-env --allow-read=./ --allow-write=./_api --allow-net=content.dollyskettle.com download-images-deno.js",

    "comment__install-web-modules": "https://github.com/denoland/deno/issues/19162",
    "comment__install-web-modules_2": "https://deno.com/manual@v1.33.4/tools/vendor",
    "not-implemented__install-web-modules": "deno run --allow-env --allow-read --allow-write=./web_modules install-web-modules.js"
  }
}
